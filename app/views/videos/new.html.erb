<p>ルーティンに使用したい動画をYouTubeから選択肢しよう！</p>

<button class="btn btn-secondary">YouTubeへ</button>

    <%= form_with model: @video, local: true, url: {controller: 'videos', action: 'create' } do |f| %>
       <%= render 'layouts/error_messages', model: f.object %>
       
      <div class="form-group">
        <%= f.label :url, '動画のリンク' %>
        <%= f.text_field :url, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :start_time, 'ルーティンの始まり' %>
        <%= f.text_field :start_time, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :end_time, 'ルーティンの終わり' %>
        <%= f.text_field :end_time, class: 'form-control' %>
      </div>

      <%= f.submit 'この動画をルーティンに追加！', class: 'btn btn-primary btn-block' %>
      
      <%= link_to 'ビデオの追加を終了する！', routine_path(@routine) %>
    <% end %>
